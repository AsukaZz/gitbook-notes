{
  "title": "个人笔记",
  "author": "Willian He",
  "description": "个人笔记",
  "keywords": "个人笔记,willian he,",
  "introduction": {
    "path": "README.md",
    "title": "个人笔记"
  },
  "plugins": [
    "image-captions",
    "splitter",
    "expandable-chapters-small",
    "-mathjax",
    "ga",
    "-disqus",
    "-lunr",
    "-search",
    "search-pro",
    "localized-footer"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "image-captions": {
      "caption": "Image - _CAPTION_",
      "align": "center"
    },
    "expandable-chapters-small": {},
    "fontSettings": {
      "theme": "white",
      "family": "serif",
      "size": 2
    },
    "ga": {
      "token": "UA-60753376-1"
    },
    "disqus": {
      "shortName": "gitbook-tutorial"
    },
    "search-pro": {
      "cutWordLib": "nodejieba"
    }
  },
  "links": {
    "home": false,
    "about": false,
    "issues": false,
    "contribute": false,
    "tail": {
      "": ""
    },
    "gitbook": false,
    "sharing": {
      "google": true,
      "facebook": true,
      "twitter": true,
      "weibo": true,
      "qrcode": true
    }
  },
  "pdf": {
    "toc": true,
    "pageNumbers": true,
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 36,
      "bottom": 36
    }
  }
}
